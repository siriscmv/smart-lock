generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

model users {
    username String @id
    password String @unique
    type EnumUserType
}

model vehicles {
    vid String @id
    vname String @unique
}

model vehicles_location {
    vid String @id
    latitude Float
    longitude Float
    type EnumLocationType
}

enum EnumUserType {
    OWNER
    DRIVER
}

enum EnumLocationType {
    START
    TOLL
    END
}